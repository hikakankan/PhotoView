<Window x:Class="PhotoSelect.TextDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PhotoSelect"
        mc:Ignorable="d"
        Title="名前の変更" Height="133.555" Width="323.92">
    <Grid>
        <Canvas Margin="0">
            <TextBox x:Name="textBoxName" Height="23" Canvas.Left="25" TextWrapping="Wrap" Canvas.Top="22" Width="177"/>
            <Button x:Name="buttonOK" Content="OK" Canvas.Left="25" Canvas.Top="64" Width="75" Click="buttonOK_Click"/>
            <Button x:Name="buttonCancel" Content="Cancel" Canvas.Left="126" Canvas.Top="64" Width="75" Click="buttonCancel_Click"/>
        </Canvas>

    </Grid>
</Window>
